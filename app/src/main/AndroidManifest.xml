<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.afiadorcerebral.afiadorcerebral">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AfiadorCerebral">
        <activity android:name=".HistoricoActivity"></activity>
        <activity android:name=".SelecaoActivity" />
        <activity android:name=".ConfiguracoesActivity" />
        <activity android:name=".GameActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>